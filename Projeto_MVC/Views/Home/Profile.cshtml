<div class="card" style="width: 18rem; margin: auto; text-align: center; border-radius: 15px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);">

    <div style="text-align: left"> 
        <button style="border:none; background:none; padding:0; margin-left: 20px; cursor:pointer;" onclick="location.href='/Home/Services'">
            <svg xmlns="http://www.w3.org/2000/svg" class="return" width="16" height="16" fill="currentColor" class="bi bi-arrow-left-circle" viewBox="0 0 16 16" >
                <path fill-rule="evenodd" d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8m15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-4.5-.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5z" />
            </svg>
        </button>
    </div>
    <img src="data:image/png;base64," class="card-img-top rounded-circle mx-auto mt-3" alt="Imagem do Usuário" style="width: 100px; height: 100px;">
    <div class="card-body">
        <h5 class="card-title">@ViewBag.FullName</h5>
        <p class="card-text">Usuário da Versão de Desenvolvimento</p>
        <p class="card-text">@ViewBag.UserEmail</p>
        <button type="submit" class="btn btn-primary rounded-pill px-3 mt-2" onclick="location.href='/Home/Edit'">Editar Perfil</button>
    </div>
</div>

